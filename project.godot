; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Bubble Drop - Beta"
run/main_scene="res://scenes/gamescene.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://GameIcon.svg"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"

[dotnet]

project/assembly_name="_Godot1"

[editor_plugins]

enabled=PackedStringArray("res://addons/leveleditorplugin/plugin_scripts/plugin.cfg", "res://addons/radialcontainer_plugin/plugin.cfg", "res://addons/trail2D_plugin/plugin.cfg")

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="Bubbles"
2d_physics/layer_2="Bubble Trigger"
2d_physics/layer_3="Sling"
2d_physics/layer_4="Metal"

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
